@model mvc_project.Models.companyinsert
@{
    ViewBag.Title = "insertcompany_pageload";
}

<h2>company_pageload</h2>
<div>
    @using (Html.BeginForm("insertcompany_click", "companyreg", FormMethod.Post))
    {
        <div>
            <label>name:</label>
        </div>
        <div>
            @Html.TextBoxFor(m => m.name)
            @Html.ValidationMessageFor(m => m.name)
        </div>
        <div>
            <label>address:</label>
        </div>
        <div>
            @Html.TextBoxFor(m => m.address)
            @Html.ValidationMessageFor(m => m.address)

        </div>
        <div>
            <label>phone:</label>
        </div>
        <div>
            @Html.TextBoxFor(m => m.phone)
            @Html.ValidationMessageFor(m => m.phone)
        </div>
        <div>
            <label>email:</label>
        </div>
        <div>
            @Html.TextBoxFor(m => m.email)
            @Html.ValidationMessageFor(m => m.email)
        </div>
        <div>
            <label>status:</label>
        </div>
        <div>
            @Html.TextBoxFor(m => m.status)
           
        </div>

        <div>
            <label>username:</label>
        </div>
        <div>
            @Html.TextBoxFor(m => m.username)
            @Html.ValidationMessageFor(m => m.username)

        </div>
        <div>
            <label>password:</label>
        </div>
        <div>
            @Html.PasswordFor(m => m.pass)

        </div>
        <div>
            <label>confirm password:</label>
        </div>
        <div>
            @Html.PasswordFor(m => m.cpassword)
            @Html.ValidationMessageFor(m => m.cpassword)
        </div>
        <div>
            <input type="submit" value="insert" />
            @Html.DisplayFor(m => m.msg);
        </div>



    }
</div>


